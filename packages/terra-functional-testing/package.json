{
  "private": true,
  "name": "@cerner/terra-functional-testing",
  "version": "0.0.0",
  "description": "A functional testing utility for applications built using Terra.",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && babel ./src -d lib",
    "lint": "eslint --ext .js,.jsx .",
    "jest": "jest --config ../../jest.config.js",
    "prebuild": "rm -rf lib/services/wdio-terra-visual-regression",
    "prepare": "npm run build",
    "mocha": "mocha --require @babel/register test/mocha/**/*.test.jss",
    "test": "npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-toolkit",
    "directory": "packages/terra-functional-testing"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Toolkit",
    "terra-toolkit",
    "terra-functional-testing"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-toolkit/issues"
  },
  "homepage": "https://github.com/cerner/terra-toolkit#readme",
  "bin": {
    "tt-test": "bin/test-runner.js"
  },
  "dependencies": {
    "@wdio/cli": "^6.1.4",
    "@wdio/local-runner": "^6.1.4",
    "@wdio/logger": "^6.0.16",
    "@wdio/mocha-framework": "^6.1.0",
    "@wdio/spec-reporter": "^6.0.16",
    "@wdio/sync": "^6.1.14",
    "axe-core": "3.5.5",
    "commander": "^5.1.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "gm": "^1.23.1",
    "image-size": "^0.8.3",
    "ip": "^1.1.5",
    "jimp": "^0.13.0",
    "lodash": "^4.17.19",
    "node-resemble-js": "0.2.0",
    "nodeclient-spectre": "^1.0.5",
    "platform": "^1.3.6",
    "uuid": "^8.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
