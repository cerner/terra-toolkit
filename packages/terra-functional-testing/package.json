{
  "name": "@cerner/terra-functional-testing",
  "version": "4.8.0",
  "description": "A functional testing utility for applications built using Terra.",
  "author": "Cerner Corporation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-toolkit",
    "directory": "packages/terra-functional-testing"
  },
  "bugs": {
    "url": "https://github.com/cerner/terra-toolkit/issues"
  },
  "homepage": "https://github.com/cerner/terra-toolkit#readme",
  "license": "Apache-2.0",
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Toolkit",
    "terra-functional-testing",
    "terra-toolkit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "12 || 14",
    "npm": "6 || 9"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "dependencies": {
    "@babel/runtime": "7",
    "@mirzazeyrek/node-resemble-js": "1",
    "@octokit/core": "3",
    "@wdio/cli": "7",
    "@wdio/local-runner": "7",
    "@wdio/mocha-framework": "7",
    "@wdio/reporter": "7",
    "@wdio/spec-reporter": "7",
    "@wdio/sync": "7",
    "archiver": "4",
    "axe-core": "4.0.2",
    "chalk": "4",
    "expect": "26",
    "express": "4",
    "extract-zip": "2",
    "form-data": "3",
    "fs-extra": "9",
    "glob": "7",
    "gm": "1",
    "image-size": "^0.8.3",
    "inquirer": "8.1.3",
    "ip": "1",
    "jimp": "^0.13.0",
    "js-yaml": "3",
    "lodash.get": "4",
    "lodash.identity": "3",
    "lodash.isplainobject": "4",
    "lodash.isstring": "4",
    "lodash.isundefined": "3",
    "lodash.pickby": "4",
    "node-fetch": "2",
    "octokit": "1",
    "strip-ansi": "6",
    "uuid": "3.4.0",
    "webdriverio": "7",
    "webpack-dev-server": "4",
    "which": "2"
  },
  "peerDependencies": {
    "@cerner/terra-cli": "^1.11.0",
    "webpack": "4 || 5",
    "webpack-cli": "3 || 4"
  },
  "devDependencies": {
    "@babel/cli": "7",
    "@babel/core": "7",
    "@babel/helpers": "7",
    "@babel/plugin-proposal-object-rest-spread": "7",
    "@babel/plugin-syntax-async-generators": "7",
    "@babel/plugin-transform-regenerator": "7",
    "@babel/plugin-transform-runtime": "7",
    "@babel/preset-env": "7",
    "@babel/preset-react": "7",
    "@babel/register": "7",
    "@cerner/terra-cli": "^1.12.0",
    "@octokit/core": "3",
    "chai": "4",
    "eslint": "7",
    "html-webpack-plugin": "4",
    "lodash.groupby": "4",
    "lodash.map": "4",
    "lodash.mapkeys": "4",
    "lodash.mapvalues": "4",
    "memory-fs": "^0.5.0",
    "mocha": "10",
    "nock": "12",
    "np": "6",
    "sinon": "9",
    "yargs": "16"
  },
  "scripts": {
    "clean": "npm run clean:node_modules && npm run clean:lib",
    "clean:lib": "rm -rf lib",
    "clean:node_modules": "echo 'removing ./node_modules' && rm -rf node_modules && echo 'removed ./node_modules'",
    "clean:install": "npm run clean && time npm install",
    "compile": "babel --root-mode upward src --out-dir lib --copy-files --ignore src/terra-cli",
    "lint": "eslint --ext .js,.jsx --ignore-path ../../.eslintignore .",
    "lint:js:fix": "eslint --ext .js,.jsx --ignore-path ../../.eslintignore . --fix",
    "jest": "jest",
    "premocha": "rm -rf tests/tmp",
    "mocha": "MOCHA=true mocha --require @babel/register tests/mocha/**/unit/**/*.test.js",
    "test": "npm run jest && npm run mocha"
  }
}
