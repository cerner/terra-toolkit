{
  "private": true,
  "name": "@cerner/terra-functional-testing",
  "version": "0.0.0",
  "description": "A functional testing utility for applications built using Terra.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "NOTICE",
    "LICENSE",
    "*.md"
  ],
  "scripts": {
    "compile": "babel src --out-dir lib --copy-files",
    "lint": "eslint --ext .js,.jsx --ignore-path ../../.eslintignore .",
    "jest": "jest --config ../../jest.config.js",
    "precompile": "rm -rf lib",
    "prepare": "npm run compile",
    "premocha": "rm -rf tests/tmp",
    "mocha": "MOCHA=true mocha --require @babel/register tests/mocha/**/unit/**/*.test.js",
    "test": "npm run jest && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-toolkit",
    "directory": "packages/terra-functional-testing"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Toolkit",
    "terra-toolkit",
    "terra-functional-testing"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-toolkit/issues"
  },
  "homepage": "https://github.com/cerner/terra-toolkit#readme",
  "bin": {
    "tt-test": "bin/test-runner.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@wdio/cli": "^6.1.4",
    "@wdio/local-runner": "^6.1.4",
    "@wdio/logger": "^6.0.16",
    "@wdio/mocha-framework": "^6.1.0",
    "@wdio/spec-reporter": "^6.0.16",
    "@wdio/sync": "^6.1.14",
    "axe-core": "3.5.5",
    "commander": "^5.1.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "gm": "^1.23.1",
    "image-size": "^0.8.3",
    "ip": "^1.1.5",
    "jimp": "^0.13.0",
    "lodash": "^4.17.19",
    "node-resemble-js": "0.2.0",
    "platform": "^1.3.6",
    "uuid": "^3.0.0",
    "webdriverio": "^6.5.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.1",
    "nock": "^12.0.3",
    "np": "^6.3.2",
    "sinon": "^9.0.2"
  }
}
